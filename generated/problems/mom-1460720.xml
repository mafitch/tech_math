<table><tabular> <row><cell>Year</cell><cell>Dawn</cell><cell>Teyonah</cell><cell>Joseph</cell></row><row><cell>2019</cell><cell>65791</cell><cell>65970</cell><cell>56559.36</cell></row><row><cell>2020</cell><cell>66448.91</cell><cell>68608.8</cell><cell>58206.72</cell></row><row><cell>2021</cell><cell>67113.4</cell><cell>71353.15</cell><cell>59854.08</cell></row><row><cell>2022</cell><cell>67784.53</cell><cell>74207.28</cell><cell>61501.44</cell></row><row><cell>2023</cell><cell>68462.38</cell><cell>77175.57</cell><cell>63148.8</cell></row><row><cell>2024</cell><cell>69147</cell><cell>80262.59</cell><cell>64796.16</cell></row></tabular></table>
