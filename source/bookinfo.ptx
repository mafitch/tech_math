<docinfo>

    <!-- the other option is "long" which will produce an -->
    <!-- entire front matter section with more headings   -->
    <author-biographies length="short" />

    <brandlogo url="http://uaa.alaska.edu" source="images/UAA_GG.png" />

    <!-- this is the default, but supresses a warning -->
    <cross-references text="type-global" />
    <latex-preamble>
        <package>cancel</package>
    </latex-preamble>
    <macros>
    %  cancelhighlight (draw attention to when a factor is canceled)
    %  Usage: \frac{(x+1)\cancelhighlight{(x+2)}{\cancelhighlight{(x+2)}}}
    \newcommand{\cancelhighlight}[1]{\color{blue}{{\cancel{#1}}}}
    %
    %
    %  secondcancelhighlight (draw attention to when a factor is canceled with a second color)
    %  Usage: \frac{(x+1)\secondcancelhighlight{(x+2)}{\secondcancelhighlight{(x+2)}}}
    \newcommand{\secondcancelhighlight}[1]{\color{green}{{\bcancel{#1}}}}
    %
    %
    %  thirdcancelhighlight (draw attention to when a factor is canceled with a third color)
    %  Usage: \frac{(x+1)\thirdcancelhighlight{(x+2)}{\thirdcancelhighlight{(x+2)}}}
    \newcommand{\thirdcancelhighlight}[1]{\color{orange}{{\xcancel{#1}}}}
    </macros>

</docinfo>
